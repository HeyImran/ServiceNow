var user = new GlideRecord('sys_user');
user.orderBy('name');
//user.setLimit();
user.query();
var allUSers = [];
while (user.next()){
	allUSers.push(user.name.getDisplayValue());
}
gs.info("The Number of Users are : "+ allUSers.length.toString());
gs.info(allUSers.join(','));


